{
  "name": "swifty-frontend",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "bank": "turbo run dev --filter=swifty-bank",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "test": "turbo test"
  },
  "devDependencies": {
    "@swifty/eslint-config": "*",
    "@swifty/prettier-config": "*",
    "@swifty/typescript-config": "*",
    "eslint-plugin-turbo": "^1.11.3",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "yarn@4.0.2",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "framer-motion": "^11.0.3",
    "next-themes": "^0.2.1"
  }
}
